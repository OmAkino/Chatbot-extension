{
    "manifest_version": 3,
    "name": "URL Reader Extension",
    "version": "1.0",
    "description": "Reads and displays the current page URL.",
    "permissions": ["tabs", "activeTab", "storage"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "host_permissions": ["<all_urls>"]
  }
  